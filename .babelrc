{
	"presets": [
		[
			"env",
			{
				"targets": {
					"browsers": [
						">1%",
						"last 4 versions",
						"Firefox ESR",
						"not ie < 9"
					]
				},
				"modules": false
			}
		]
	],
	"plugins": [
		"@babel/plugin-syntax-dynamic-import"
	],
	"ignore": [
		"node_modules",
		"dist"
	]
}

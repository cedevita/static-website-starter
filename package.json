{
  "name": "static-website-starter",
  "version": "1.0.0",
  "description": "Static website starter - Webpack, Bootstrap, Handlebars",
  "main": "index.js",
  "scripts": {
    "cleardist": "rimraf ./dist/*",
    "lint": "xo *.js",
    "start": "node_modules/.bin/webpack-dev-server",
    "build": "yarn cleardist && NODE_ENV=production webpack -p"
  },
  "author": "Mladen Ruzicic <ruzicic@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "bootstrap": "v4.0.0-alpha.6",
    "bootstrap-loader": "2.1.0",
    "css-loader": "0.28.4",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "0.11.2",
    "global": "4.3.2",
    "handlebars": "4.0.10",
    "handlebars-loader": "1.5.0",
    "html-webpack-plugin": "2.29.0",
    "image-webpack-loader": "3.3.1",
    "imports-loader": "^0.7.1",
    "node-sass": "4.5.3",
    "postcss": "^6.0.6",
    "postcss-loader": "^2.0.6",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "resolve-url-loader": "2.1.0",
    "resource-hints-webpack-plugin": "^0.0.1",
    "rimraf": "2.6.1",
    "sass-loader": "6.0.6",
    "style-loader": "0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "3.1.0",
    "webpack-dev-server": "2.5.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "esnext": true,
    "rules": {
      "import/no-unresolved": 0,
      "import/no-extraneous-dependencies": 0
    }
  },
  "keywords": [
    "webpack",
    "handlebars",
    "bootstrap"
  ]  
}